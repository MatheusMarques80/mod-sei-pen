<?xml version='1.0' encoding='UTF-8'?><schema xmlns:com="http://pen.planejamento.gov.br/interoperabilidade/soap/v1_1/comum" xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://pen.planejamento.gov.br/interoperabilidade/soap/v1_1/comum">
  
  <simpleType name="NRE">
    <annotation>
      <documentation>
        Número de Registro Eletrônico. Identificador
        do processo ou documento tramitado, que permite
        relacionar futuros trâmites do mesmo objeto
        na solução de interoperabilidade. Seu formato é
        composto de dezesseis dígitos numéricos, onde os
        10 primeiros são um sequencial, os 2 próximos
        o dígito verificador (calculado a partir do
        sequencial) e os 4 últimos se referem ao ano
        de geração do número.
      </documentation>
    </annotation>
    <restriction base="string">
      <pattern value="\d{16}"/>
    </restriction>
  </simpleType>
  
  <simpleType name="IDT">
  	<annotation>
  	  <documentation>Identificador do trâmite</documentation>
  	</annotation>
  	<restriction base="long"/>
  </simpleType>
  
  <simpleType name="justificativaDaRecusaDeTramite">
    <restriction base="string">
       <minLength value="1"/>
       <maxLength value="1000"/>
    </restriction>
  </simpleType>
  
  <simpleType name="motivoDaRecusaDeTramite">
	  <restriction base="string">
	    <enumeration value="01"><annotation><documentation>Formato de componente digital não suportado</documentation></annotation></enumeration>
	    <enumeration value="02"><annotation><documentation>Componente digital corrompido</documentation></annotation></enumeration>
	    <enumeration value="03"><annotation><documentation>Falta de componentes digitais</documentation></annotation></enumeration>
	    <enumeration value="99"><annotation><documentation>Outro</documentation></annotation></enumeration>
	  </restriction>
  </simpleType>
  
  <simpleType name="valorPropriedadeAdicional">
    <restriction base="string">
      <minLength value="1"/>
      <maxLength value="10000"/>
    </restriction>
  </simpleType>
  
  <complexType name="propriedadeAdicional">
    <annotation>
      <documentation>
        Tipo de dados utilizado para permitir que
        o sistema informe dados adicionais, com
        o fim de aumentar as capacidades de interação
        entre as pontas.
      </documentation>
    </annotation>
    <simpleContent>
     <extension base="com:valorPropriedadeAdicional">
       <attribute name="chave" use="required">
         <simpleType>
           <restriction base="string">
             <minLength value="1"/>
             <maxLength value="250"/>
           </restriction>
         </simpleType>
       </attribute>
     </extension>
    </simpleContent>
  </complexType>
  
  <simpleType name="nome">
    <annotation>
      <documentation>
        Define a estrutura de um nome genérico.
      </documentation>
    </annotation>
    <restriction base="string">
      <minLength value="1"/>
      <maxLength value="150"/>
    </restriction>
  </simpleType>
  
  <simpleType name="protocolo">
    <annotation>
      <documentation>
        Número de protocolo da entidade em tramitação.
      </documentation>
    </annotation>
    <restriction base="string">
      <minLength value="1"/>
      <maxLength value="40"/>
    </restriction>
  </simpleType>
  
  <simpleType name="nivelDeSigilo">
    <annotation>
      <documentation>
        Nível de sigilo do artefato. Até o presente momento
        apenas artefatos públicos serão aceitos. Em futuras
        versões os outros níveis passarão a ser permitidos.
      </documentation>
    </annotation>
    <restriction base="integer">
      <enumeration value="1"><annotation><documentation>Público</documentation></annotation></enumeration>
    </restriction>
  </simpleType>
  
  <complexType name="produtor">
    <annotation>
      <documentation>
        Entidade produtora do artefato em questão.
        Pode ou não estar presente nos repositórios
        de estruturas do serviço.
      </documentation>
    </annotation>
    <complexContent>
      <extension base="com:pessoa">
        <sequence>
		      <element minOccurs="0" name="unidade">
		        <annotation>
		          <documentation>
		            Detalhes da unidade, dentro da entidade produtora,
		            que de fato produziu o documento. Pode ou não
		            ser informada.
		          </documentation>
		        </annotation>
		        <complexType>
			        <complexContent>
			          <extension base="com:pessoa"/>
			        </complexContent>
		        </complexType>
		      </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  
