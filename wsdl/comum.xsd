      <element minOccurs="0" name="numero" type="integer"/>
      <element minOccurs="0" name="ano" type="integer"/>
      <element minOccurs="0" name="siglaDaUnidadeProdutora">
        <annotation>
          <documentation>
            Sigla da unidade administrativa que produziu
            determinado documento de uma espécie documental.
          </documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="complemento">
        <simpleType>
