      <element name="codigo">
        <simpleType>
          <restriction base="string">
            <enumeration value="01"><annotation><documentation>Registro</documentation></annotation></enumeration>
            <enumeration value="02"><annotation><documentation>Envio de documento avulso/processo</documentation></annotation></enumeration>
            <enumeration value="03"><annotation><documentation>Cancelamento/exclusão ou envio de documento</documentation></annotation></enumeration>
            <enumeration value="04"><annotation><documentation>Recebimento de documento</documentation></annotation></enumeration>
            <enumeration value="05"><annotation><documentation>Autuação</documentation></annotation></enumeration>
            <enumeration value="06"><annotation><documentation>Juntada por anexação</documentation></annotation></enumeration>
            <enumeration value="07"><annotation><documentation>Juntada por apensação</documentation></annotation></enumeration>
            <enumeration value="08"><annotation><documentation>Desapensação</documentation></annotation></enumeration>
            <enumeration value="09"><annotation><documentation>Arquivamento</documentation></annotation></enumeration>
            <enumeration value="10"><annotation><documentation>Arquivamento no Arquivo Nacional</documentation></annotation></enumeration>
            <enumeration value="11"><annotation><documentation>Eliminação</documentation></annotation></enumeration>
            <enumeration value="12"><annotation><documentation>Sinistro</documentation></annotation></enumeration>
            <enumeration value="13"><annotation><documentation>Reconstituição de processo</documentation></annotation></enumeration>
            <enumeration value="14"><annotation><documentation>Desarquivamento</documentation></annotation></enumeration>
            <enumeration value="15"><annotation><documentation>Desmembramento</documentation></annotation></enumeration>
            <enumeration value="16"><annotation><documentation>Desentranhamento</documentation></annotation></enumeration>
            <enumeration value="17"><annotation><documentation>Encerramento/abertura de volume no processo</documentation></annotation></enumeration>
            <enumeration value="18"><annotation><documentation>Registro de extravio</documentation></annotation></enumeration>
          </restriction>
        </simpleType>
      </element>
      <element name="dataHora" type="dateTime"/>
      <element name="complemento">
        <simpleType>
          <restriction base="string">
            <maxLength value="500"/>
          </restriction>
        </simpleType>
      </element>
    </sequence>
  </complexType>
  
  <complexType name="documentoDeIdentificacao">
    <annotation>
      <documentation>
        Define um documento de identificação de
        uma determinada pessoa.
      </documentation>
    </annotation>
    <sequence>
      <element name="codigo">
        <simpleType>
          <restriction base="string">
            <minLength value="1"/>
            <maxLength value="50"/>
          </restriction>
        </simpleType>
      </element>
      <element name="emissor" type="com:nome"/>
      <element name="tipo">
        <simpleType>
          <restriction base="string">
						<enumeration value="CI"><annotation><documentation>Carteira de identidade</documentation></annotation></enumeration>
						<enumeration value="CNH"><annotation><documentation>Carteira nacional de habilitação</documentation></annotation></enumeration>
						<enumeration value="TE"><annotation><documentation>Título de eleitor</documentation></annotation></enumeration>
						<enumeration value="CN"><annotation><documentation>Certidão de nascimento</documentation></annotation></enumeration>
						<enumeration value="CC"><annotation><documentation>Certidão de casamento</documentation></annotation></enumeration>
						<enumeration value="PAS"><annotation><documentation>Passaporte</documentation></annotation></enumeration>
						<enumeration value="CT"><annotation><documentation>Carteira de trabalho</documentation></annotation></enumeration>
						<enumeration value="RIC"><annotation><documentation>Registro individual do cidadão</documentation></annotation></enumeration>
						<enumeration value="CMF"><annotation><documentation>Cadastro do ministério da fazenda</documentation></annotation></enumeration>
						<enumeration value="PISPASEP"><annotation><documentation>Número no programa de integração social</documentation></annotation></enumeration>
						<enumeration value="CEI"><annotation><documentation>Cadastro específico do INSS</documentation></annotation></enumeration>
						<enumeration value="NIT"><annotation><documentation>Número de identificação do trabalho</documentation></annotation></enumeration>
						<enumeration value="CP"><annotation><documentation>Cadastro em conselhos profissionais</documentation></annotation></enumeration>
						<enumeration value="IF"><annotation><documentation>Identidade funcional</documentation></annotation></enumeration>
						<enumeration value="OAB"><annotation><documentation>Ordem dos advogados do Brasil</documentation></annotation></enumeration>
						<enumeration value="RJC"><annotation><documentation>Número de inscrição empresarial</documentation></annotation></enumeration>
						<enumeration value="RGE"><annotation><documentation>Registro de identificação do estrangeiro</documentation></annotation></enumeration>
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="nomeNoDocumento" type="com:nome"/>
    </sequence>
  </complexType>
  
  <complexType name="endereco">
    <annotation>
      <documentation>
        Define um endereço, que pode ser
        de residência de uma pessoa física,
        sede de uma pessoa jurídica, entre
        outras possibilidades.
      </documentation>
    </annotation>
    <sequence>
      <element minOccurs="0" name="logradouro">
        <simpleType>
          <restriction base="string">
            <minLength value="1"/>
            <maxLength value="250"/>
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="numero">
        <simpleType>
          <restriction base="string">
            <minLength value="1"/>
            <maxLength value="50"/>
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="complemento">
        <simpleType>
          <restriction base="string">
            <minLength value="1"/>
            <maxLength value="250"/>
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="bairro" type="com:nome"/>
      <element minOccurs="0" name="cidade" type="com:nome"/>
      <element minOccurs="0" name="estado">
        <simpleType>
          <restriction base="string">
            <pattern value="[A-Z]{2}"/>
          </restriction>
        </simpleType>
      </element>
      <element minOccurs="0" name="pais" type="com:nome"/>
      <element minOccurs="0" name="cep">
        <simpleType>
          <restriction base="string">
            <pattern value="\d{8}"/>
          </restriction>
        </simpleType>
      </element>
    </sequence>
  </complexType>
  
  <simpleType name="poloDoInteressado">
    <annotation>
      <documentation>
        Pólo que o interessado assume
        no processo. Indica o nível
        de interesse do mesmo com o
        desfecho do mesmo.
      </documentation>
    </annotation>
    <restriction base="string">
      <enumeration value="ativo"/>
      <enumeration value="passivo"/>
      <enumeration value="terceiro"/>
    </restriction>
  </simpleType>
  
  <complexType name="interessado">
    <annotation>
      <documentation>
        Pessoa que possui algum interesse
        no desfecho do processo administrativo
        relacionado.
      </documentation>
    </annotation>
    <complexContent>
      <extension base="com:pessoa">
        <sequence>
          <element minOccurs="0" name="polo" type="com:poloDoInteressado"/>
          <element maxOccurs="unbounded" minOccurs="0" name="outroNome" type="com:nome"/>
          <element maxOccurs="unbounded" minOccurs="0" name="documentoDeIdentificacao" type="com:documentoDeIdentificacao"/>
          <element maxOccurs="unbounded" minOccurs="0" name="endereco" type="com:endereco"/>
          <element minOccurs="0" name="sexo">
            <simpleType>
              <restriction base="string">
                <enumeration value="M"><annotation><documentation>Masculino</documentation></annotation></enumeration>
                <enumeration value="F"><annotation><documentation>Feminino</documentation></annotation></enumeration>
                <enumeration value="D"><annotation><documentation>Desconhecido</documentation></annotation></enumeration>
              </restriction>
            </simpleType>
          </element>
          <element minOccurs="0" name="nomeDoGenitor" type="com:nome"/>
          <element minOccurs="0" name="nomeDaGenitora" type="com:nome"/>
          <element minOccurs="0" name="dataDeNascimento" type="date"/>
          <element minOccurs="0" name="dataDeObito" type="date"/>
          <element minOccurs="0" name="cidadeNatural" type="com:nome"/>
          <element minOccurs="0" name="estadoNatural">
            <simpleType>
              <restriction base="string">
                <pattern value="[A-Z]{2}"/>
              </restriction>
            </simpleType>
          </element>
          <element minOccurs="0" name="nacionalidade">
            <annotation>
              <documentation>
                País da nacionalidade da pessoa individualizada, no
                formato ISO-3166-1-alpha-2.
              </documentation>
            </annotation>
            <simpleType>
              <restriction base="string">
                <pattern value="[A-Z]{2}"/>
              </restriction>
            </simpleType>
          </element>
        </sequence>
      </extension>
    </complexContent>
  </complexType>
  
  <simpleType name="motivoDaUrgencia">
	<annotation>
		<documentation>
			Caso o processo seja marcado como "urgente",
			este campo passa a ser de preenchimento obrigatório.
		</documentation>
	</annotation>    				  
    <restriction base="string">
	  <!-- Nota para manutenção: qualquer alteração nessa enumeração
	  de valores *deve* ser refletida no mecanismo que valida a entrada
	  de dados da API REST (validador JSR303). -->
      <enumeration value="1"><annotation><documentation>hipótese de prioridade prevista em lei</documentation></annotation></enumeration>
      <enumeration value="2"><annotation><documentation>assunto relativo ao cumprimento de decisão judicial</documentation></annotation></enumeration>
      <enumeration value="3"><annotation><documentation>risco de perecimento de direito do interessado</documentation></annotation></enumeration>
      <enumeration value="4"><annotation><documentation>risco de prescrição ou decadência de pretensão da Administração Pública</documentation></annotation></enumeration>
      <enumeration value="5"><annotation><documentation>relevante interesse público</documentation></annotation></enumeration>
    </restriction>
  </simpleType>
  
</schema>